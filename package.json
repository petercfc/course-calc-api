{
  "name": "example",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts",
    "start": "npm run serve",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "start:dev": "ts-node-dev --no-notify --respawn --transpileOnly ./src"
  },
  "dependencies": {
    "graphql": "14.1.1",
    "graphql-yoga": "1.17.4",
    "nexus": "latest",
    "nexus-prisma": "latest",
    "prisma-client-lib": "latest"
  },
  "devDependencies": {
    "@types/graphql": "14.0.7",
    "@types/node": "10.12.30",
    "nexus-prisma-generate": "latest",
    "ts-node": "8.0.3",
    "ts-node-dev": "1.0.0-pre.32",
    "typescript": "3.3.3333"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "printWidth": 80,
    "trailingComma": "all",
    "semi": false
  }
}
